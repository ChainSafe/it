  "main": "index.js",
  "repository": "github:achingbrain/it",
  "homepage": "https://github.com/achingbrain/it#readme",
  "bugs": "https://github.com/achingbrain/it/issues",
  "scripts": {
    "test": "ava",
    "lint": "standard",
